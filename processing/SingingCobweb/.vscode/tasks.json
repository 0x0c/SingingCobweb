        "label": "Run",
        "command": "/usr/local/bin/processing-java",
        "type": "process",
        "args": [
          "--force",
          "--sketch=${workspaceRoot}",
          "--run"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "presentation": {
          "reveal": "always",
          "panel": "new"
        }
      }
